        }, {
            key: 'setVolume',
            value: function setVolume(volume) {
                return this.set('volume', volume);
            }
        }]);

        return Player;
    }();

    if (!isNode) {
        initializeEmbeds();
        resizeEmbeds();
    }

    return Player;

