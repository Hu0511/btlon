	if(document.cookie.indexOf('zenfo_validation') > -1) cookieExpires(0, 'zenfo_validation');
}

if(location.pathname.indexOf("/ipl-customer/ipl/search") > -1 || !!$('form.stepprint').length) {
	if(document.cookie.indexOf('zenfo_validation') > -1) cookieExpires(0, 'zenfo_validation');
}

