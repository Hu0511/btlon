      fullScreenSlider: 'm-39-fullscreen-slider',
      fullscreenDownload: 'm-39-fullscreen-download',
      fullscreenShare: 'm-39-fullscreen-share',
      fullscreenTitle: 'm-39-fullscreen-media-title',
