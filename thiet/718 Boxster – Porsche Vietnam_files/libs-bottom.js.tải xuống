GLOBAL_CONFIG.dealersearch.porschedealerLocatorURL = GLOBAL_CONFIG.dealersearch.porschedealerLocatorURL || 'http://porschedealer.com/dealer/usLocator/?zip=[[###ZIP###]]&page=search';

// popup window size config
