      fullscreenDownloadLinkAndroid: 'm-39-fullscreen-download__link--android',
      arrows: 'slick-arrow',
      topbar: 'm-39-top-bar'
    },
    // gallery items
    item: {
      any: 'gitem',
      fixed: 'm-39-gi-static', // "static" is a reserved keyword therefore "fixed" is used instead
