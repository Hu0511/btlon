var cookieExpires = function (z, name) {
	var _second = z;
	var _name = name;
	var now = new Date();
	var time = now.getTime();
	time += z * 1000;
	now.setTime(time);
	document.cookie = 
	_name + "=" + '; expires=' + now.toUTCString() + '; path=' + location.path;
}

