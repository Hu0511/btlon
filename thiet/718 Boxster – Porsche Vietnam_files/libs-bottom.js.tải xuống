      towPlusOne: 'm-39-slide__2-1',
      onePlusTwo: 'm-39-slide__1-2',
      downloadButtonSmall: 'm-39-mobile-icon-small',
      downloadButtonMedium: 'm-39-mobile-icon-medium',
      downloadButtonBig: 'm-39-mobile-icon-big',
